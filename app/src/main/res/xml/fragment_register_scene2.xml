<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/background"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="414dp"
            android:layout_height="732dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent">

        </Constraint>
        <Constraint
            android:id="@+id/email"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toBottomOf="@+id/regText"
            motion:layout_constraintHorizontal_bias="0.509"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp">
        </Constraint>
        <Constraint
            android:id="@+id/password"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="360dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toBottomOf="@+id/username"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp">

        </Constraint>
        <Constraint
            android:id="@+id/username"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toBottomOf="@+id/email"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp">

        </Constraint>
        <Constraint
            android:id="@+id/regText"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp"
            motion:layout_constraintHorizontal_bias="0.49">

        </Constraint>
        <Constraint
            android:id="@+id/regButt"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toBottomOf="@+id/password1"
            android:layout_marginTop="20dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent">

        </Constraint>
        <Constraint
            android:layout_height="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:id="@+id/complete"
            motion:layout_constraintHorizontal_bias="0.49"
            motion:layout_constraintTop_toBottomOf="@+id/regButt"
            android:layout_marginTop="20dp"
            android:visibility="invisible">

        </Constraint>
        <Constraint
            android:id="@+id/emailErr"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="30dp"
            motion:layout_constraintHorizontal_bias="0.509"
            motion:layout_constraintStart_toStartOf="parent"
            android:visibility="invisible"
            motion:layout_constraintTop_toBottomOf="@+id/regButt"
            android:layout_marginTop="10dp" />
        <Constraint
            android:id="@+id/nameErr"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="30dp"
            motion:layout_constraintTop_toBottomOf="@id/emailErr"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp"
            android:visibility="invisible"/>
        <Constraint
            android:id="@+id/passErr"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="30dp"
            android:visibility="invisible"
            motion:layout_constraintTop_toBottomOf="@id/nameErr"
            motion:layout_constraintHorizontal_bias="0.509"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp" />
        <Constraint
            android:id="@+id/password1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toBottomOf="@+id/username"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp" />
        <Constraint
            android:id="@+id/password1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toBottomOf="@+id/username"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/regButt"
            android:id="@+id/textView"
            android:layout_marginTop="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/background"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="414dp"
            android:layout_height="732dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintHorizontal_bias="0.666"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent">
        </Constraint>
        <Constraint
            android:id="@+id/email"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toBottomOf="@+id/regText"
            motion:layout_constraintHorizontal_bias="0.509"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp">
        </Constraint>
        <Constraint
            android:id="@+id/password"
            android:layout_width="360dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toBottomOf="@+id/username"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp"
            motion:layout_constraintEnd_toEndOf="parent">
        </Constraint>
        <Constraint
            android:id="@+id/username"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toBottomOf="@+id/email"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp">

        </Constraint>
        <Constraint
            android:id="@+id/regText"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp">

        </Constraint>
        <Constraint
            android:id="@+id/regButt"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toBottomOf="@+id/password1"
            android:layout_marginTop="20dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent">

        </Constraint>
        <Constraint
            android:layout_height="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:id="@+id/complete"
            motion:layout_constraintTop_toBottomOf="@+id/regButt"
            android:layout_marginTop="20dp"
            android:visibility="visible">
        </Constraint>
        <Constraint
            android:id="@+id/emailErr"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="30dp"
            motion:layout_constraintHorizontal_bias="0.509"
            motion:layout_constraintStart_toStartOf="parent"
            android:visibility="invisible"
            motion:layout_constraintTop_toBottomOf="@+id/regButt"
            android:layout_marginTop="10dp" />
        <Constraint
            android:id="@+id/nameErr"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="30dp"
            motion:layout_constraintTop_toBottomOf="@id/emailErr"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp"
            android:visibility="invisible"/>
        <Constraint
            android:id="@+id/passErr"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="30dp"
            android:visibility="invisible"
            motion:layout_constraintTop_toBottomOf="@+id/nameErr"
            motion:layout_constraintHorizontal_bias="0.509"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp" />
        <Constraint
            android:id="@+id/password1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toBottomOf="@+id/username"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp" />
        <Constraint
            android:id="@+id/password1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toBottomOf="@+id/username"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp" />
    </ConstraintSet>
</MotionScene>